{
	"repos": {
		"main": "",
		"frontend": "",
		"backend": ""
	},
	"options": {
		"single_repo": true,
		"dependencies": [
			"write-json-file",
			"load-json-file",
			"inquirer",
			"ora"
		],
		"scripts": {
			"dev": "npm run develop",
			"predevelop": "npm run configEnv",
			"develop": "(npm run develop:frontend) & npm run develop:backend",
			"develop:frontend": "cd nuxt && npm run dev",
			"develop:backend": "cd strapi && npm run develop --no-build",
			"build": "(npm run build:frontend) & npm run build:backend",
			"build:frontend": "cd nuxt && npm run build",
			"build:backend": "cd strapi && npm run build",
			"start": "",
			"start:frontend": "",
			"start:backend": "",
			"configEnv": "NODE_ENV=development node ./bin/build-env.js"
		}
	},
	"backend": {
		"path": "strapi",
		"extra_packages": [
			"dotenv",
			"find-config",
			"github:jasenmichael/strapi-provider-upload-sftp"
		],
		"tweaks": []
	},
	"frontend": {
		"path": "nuxt",
		"extra_packages": [],
		"tweaks": []
	}
}
